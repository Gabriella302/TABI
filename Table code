
# importing a library
import pandas as pd



#create table with headings and format
TABI = pd.DataFrame ({
    "Sainsburys sortcode": pd.Series( dtype = "int8"),
    "Sainburys account num": pd.Series (dtype = "int8"),
    "Sainsburys product type": pd.Series (dtype = "string"),
    "Natwest sortcode":  pd.Series( dtype = "int8"),
    Natwest account num": pd.Series( dtype = "int8"),
    Natwest product type": pd.Series( dtype = "string")
})


### SQL

CREATE TABLE TABI (
    Sainsburys_sortcode INT NOT NULL,
    Sainsburys_account_num INT NOT NULL,
    Sainsburys_product_type VARCHAR(50) NOT NULL,
    Natwest_sortcode INT,
    Natwest_account_num INT,
    Natwest_product_type VARCHAR(50)
);


